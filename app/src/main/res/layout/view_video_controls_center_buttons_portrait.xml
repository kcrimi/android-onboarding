<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/view_video_controls">

    <ImageView
        android:id="@+id/video_action_button"
        style="@style/SkStyle.Icon.Large"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/cast_expanded_controller_control_button_margin"
        android:layout_marginRight="@dimen/cast_expanded_controller_control_button_margin"
        android:clickable="true"
        android:focusable="true"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/player_button_pause" />

    <ImageView
        android:id="@+id/video_jump_back_button"
        style="@style/SkStyle.Icon.Large"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@id/video_action_button"
        android:clickable="true"
        android:focusable="true"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/player_button_backward_10" />

    <ImageView
        android:id="@+id/video_jump_ahead_button"
        style="@style/SkStyle.Icon.Large"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/video_action_button"
        android:clickable="true"
        android:focusable="true"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/player_button_forward_10" />
</merge>